{
  "name": "@uptimematrix/store",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "seed": "pnpm run build && node dist/prisma/seeds/seed.js"
  },
  "prisma": {
    "seed": "tsc prisma/seeds/seed.ts && node prisma/seeds/seed.js",
    "schema": "prisma/schema.prisma"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "prisma": "^6.14.0",
    "typescript": "^5.9.2"
  }
}
